[core]
	ignorecase = false
	pager = delta

[user]
	useconfigonly = true

[commit]
	gpgSign = true

[log]
	showSignature = true

[fetch]
	prune = true

[protocol]
	version = 2

[interactive]
    diffFilter = delta --color-only

[url "https://git.savannah.gnu.org/git"]
	insteadOf = git://git.savannah.gnu.org
