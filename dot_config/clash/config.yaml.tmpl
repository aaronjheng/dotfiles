mixed-port: 7890
allow-lan: true
bind-address: '*'
log-level: info
mode: rule
proxies:
  - name: {{ .clash.name }}
    type: {{ .clash.type }}
    server: {{ .clash.host }}
    port: {{ .clash.port }}
    password: {{ .clash.password }}
rules:
  - DOMAIN-SUFFIX,gstatic.com,{{ .clash.name }}
  - IP-CIDR,10.0.0.0/8,DIRECT
  - IP-CIDR,127.0.0.0/8,DIRECT
  - IP-CIDR,192.168.0.0/16,DIRECT
  - GEOIP,CN,DIRECT
  - MATCH,{{ .clash.name }}
